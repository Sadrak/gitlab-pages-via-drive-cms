# Google Drive Konfiguration
# API Key für öffentliche Drive-Ordner
GOOGLE_API_KEY=your_google_api_key_here

# Hauptordner ID in Google Drive
GOOGLE_DRIVE_FOLDER_ID=your_drive_folder_id_here

# Google Gemini AI Konfiguration
GEMINI_API_KEY=your_gemini_api_key_here

# GitLab Konfiguration
GITLAB_URL=https://gitlab.com
GITLAB_PROJECT_ID=your_project_id_here
GITLAB_ACCESS_TOKEN=your_gitlab_access_token_here
GITLAB_TARGET_BRANCH=main

# VitePress Lokale Pfade (relativ zum Repository-Root)
VITEPRESS_SRC_PATH=src
VITEPRESS_ASSETS_PATH=public/assets
